{
  "name": "Google docs control extension",
  "version": "1.0",
   "background_page": "background.html",
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "http://www.ya.ru", "http://mosgtughack.appspot.com/*"
  ],
  "browser_action": {
      "default_title": "DocExt",
      "default_icon": "icon.png",
      "popup": "popup.html"
  }
}